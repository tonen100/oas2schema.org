{
    "plugins":[
        "plugins/markdown",
        "plugins/summarize"
    ],
    "recurseDepth": 3,
    "source": {
        "include": [
            "src/errors.js",
            "src/index.js",
            "src/oasConverter.js",
            "src/oasValidator.js",
            "src/semanticTranslator.js"
        ],
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}